# Production Environment Variables
# DO NOT COMMIT ACTUAL VALUES - Use environment-specific deployment configs

# App Configuration
NODE_ENV=production
VITE_APP_URL=https://dominauts.com
VITE_APP_VERSION=2.0.0
VITE_APP_NAME=Dominauts

# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
VITE_SUPABASE_SERVICE_KEY=your-supabase-service-key

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-firebase-auth-domain
VITE_FIREBASE_PROJECT_ID=your-firebase-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-firebase-storage-bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your-firebase-messaging-sender-id
VITE_FIREBASE_APP_ID=your-firebase-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-firebase-measurement-id

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

# Analytics
VITE_GA_MEASUREMENT_ID=your-ga-measurement-id
VITE_MIXPANEL_TOKEN=your-mixpanel-token
VITE_SENTRY_DSN=your-sentry-dsn
VITE_HOTJAR_ID=your-hotjar-id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_MULTIPLAYER=true
VITE_ENABLE_TOURNAMENTS=true
VITE_ENABLE_ADS=true
VITE_ENABLE_IAP=true
VITE_ENABLE_AI=true
VITE_ENABLE_HINTS=true
VITE_ENABLE_LEADERBOARD=true
VITE_ENABLE_ACHIEVEMENTS=true
VITE_ENABLE_DAILY_CHALLENGES=true
VITE_ENABLE_SOCIAL_FEATURES=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true

# Game Configuration
VITE_MAX_PLAYERS=4
VITE_DEFAULT_GAME_MODE=allfives
VITE_TURN_TIMEOUT=30000
VITE_MAX_GAMES_PER_DAY=100
VITE_MIN_BET_AMOUNT=100
VITE_MAX_BET_AMOUNT=10000

# API Configuration
VITE_API_BASE_URL=/api
VITE_API_TIMEOUT=30000
VITE_API_RETRY_COUNT=3

# Security
VITE_ENABLE_CSP=true
VITE_CSP_REPORT_URI=https://dominauts.com/api/csp-report
VITE_ENABLE_HTTPS=true
VITE_SECURE_COOKIES=true
VITE_SESSION_TIMEOUT=3600000

# Performance
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_CACHE=true
VITE_CACHE_VERSION=v1
VITE_ENABLE_COMPRESSION=true
VITE_IMAGE_OPTIMIZATION=true

# Monitoring
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_LOG_LEVEL=error

# CDN Configuration
VITE_CDN_URL=https://cdn.dominauts.com
VITE_ASSET_PREFIX=https://cdn.dominauts.com/assets

# Email Service
RESEND_API_KEY=your-resend-api-key
EMAIL_FROM=noreply@dominauts.com

# Admin Configuration
VITE_ADMIN_EMAIL=admin@dominauts.com
VITE_SUPPORT_EMAIL=support@dominauts.com

# Rate Limiting
VITE_RATE_LIMIT_WINDOW=60000
VITE_RATE_LIMIT_MAX_REQUESTS=100

# Database
DATABASE_URL=your-database-url
DATABASE_POOL_SIZE=20

# Redis (if using for sessions/caching)
REDIS_URL=your-redis-url
REDIS_PASSWORD=your-redis-password

# Social Auth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
FACEBOOK_APP_ID=your-facebook-app-id
FACEBOOK_APP_SECRET=your-facebook-app-secret
APPLE_CLIENT_ID=your-apple-client-id
APPLE_CLIENT_SECRET=your-apple-client-secret